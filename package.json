{
  "name": "videomissileplayer",
  "version": "1.0.0",
  "description": "-------------------------------------------------- ### VideoMissilePlayer 一个无插件的`hevc`播放器",
  "scripts": {
    "start": "cp src/decoder/missile.wasm ./dist/missile.wasm && browserify play.js > ./dist/h265webjs.js && node server.js",
    "dev": "nodemon --exec 'npm start -s' --ignore bundle*"
  },
  "dependencies": {
    "browserify": "^16.5.1",
    "mp4box": "^0.3.20",
    "pinipig": "^1.6.1",
    "yuv-buffer": "^1.0.0",
    "yuv-canvas": "^1.2.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
