{
  "name": "test-h265web.js",
  "version": "1.0.0",
  "description": "H.265/Hevc Web端播放器，支持H.265编码的mp4/hls/m3u8/mpegts 的媒体播放，支持点播、直播。A Hevc Web Player , support file type: mp4/hls/m3u8/mpegts, support play type: vod/live。",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numberwolf/h265web.js.git"
  },
  "keywords": [
    "hevc",
    "player",
    "mp4",
    "hls",
    "m3u8",
    "mpegts",
    "wasm",
    "decoder",
    "wasm",
    "media",
    "play"
  ],
  "author": "numberwolf - changyanlong",
  "license": "GPL-3.0",
  "scripts": {
    "start": "browserify ./play.js -o ./dist/play_mid.js && terser ./dist/play_mid.js -c pure_funcs=[console.log],toplevel=true -m -o ./dist/play.js && node server.js",
    "dev": "nodemon --exec 'npm start -s' --ignore bundle*"
  },
  "dependencies": {
    "h265web.js": "^1.0.6",
    "webpack": "^4.44.1",
    "yuv-buffer": "^1.0.0",
    "yuv-canvas": "^1.2.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "terser-webpack-plugin": "^4.1.0"
  },
  "bugs": {
    "url": "https://github.com/numberwolf/h265web.js/issues"
  },
  "homepage": "https://github.com/numberwolf/h265web.js"
}
